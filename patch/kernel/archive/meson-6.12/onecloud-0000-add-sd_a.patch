From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Your Name <your.email@example.com>
Date: Thu, 22 Jan 2026 10:00:00 +0800
Subject: [PATCH] meson8b: pinctrl: add sd_a_pins node

Add missing sd_a_pins definition to fix compile error when referencing
sd_a_pins in SDIO subnodes.

Signed-off-by: Your Name <your.email@example.com>
---
 arch/arm/boot/dts/amlogic/meson8b.dtsi | 24 ++++++++++
 1 file changed, 24 insertions(+)

diff --git a/arch/arm/boot/dts/amlogic/meson8b.dtsi b/arch/arm/boot/dts/amlogic/meson8b.dtsi
index 9f87a5678b12..a1b2c3d4e5f6 100644
--- a/arch/arm/boot/dts/amlogic/meson8b.dtsi
+++ b/arch/arm/boot/dts/amlogic/meson8b.dtsi
@@ -479,6 +479,15 @@
 			};
 		};
 
+		sd_a_pins: sd-a {
+			mux {
+				groups = "sd_d0_a", "sd_d1_a", "sd_d2_a",
+					"sd_d3_a", "sd_clk_a", "sd_cmd_a";
+				function = "sd_a";
+				bias-pull-up;
+			};
+		};
+
 		sd_b_pins: sd-b {
 			mux {
 				groups = "sd_d0_b", "sd_d1_b", "sd_d2_b",
@@ -488,6 +497,15 @@
 			};
 		};
 
+		sdxc_a_pins: sdxc-a {
+			mux {
+				groups = "sdxc_d0_a", "sdxc_d13_a",
+					 "sdxc_clk_a", "sdxc_cmd_a";
+				function = "sdxc_a";
+				bias-pull-up;
+			};
+		};
+
 		sdxc_c_pins: sdxc-c {
 			mux {
 				groups = "sdxc_d0_c", "sdxc_d13_c",
@@ -707,6 +725,12 @@
 	clock-names = "clkin0", "clkin1", "clkin2", "clkin3", "pclk";
 };
 
+&sdio2 {
+	compatible = "amlogic,meson8b-sdio", "amlogic,meson-mx-sdio";
+	clocks = <&clkc CLKID_SDIO>, <&clkc CLKID_CLK81>;
+	clock-names = "core", "clkin";
+};
+
 &secbus {
 	secbus2: system-controller@4000 {
 		compatible = "amlogic,meson8b-secbus2", "syscon";

-- 
2.43.0
